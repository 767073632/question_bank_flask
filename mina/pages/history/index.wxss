.container{
  display: flex;
  flex-direction:column;
  align-items: center;
  background-color:#e6e6e6;
}
page{
  height:100%;
  background-color: #e6e6e6; 
}
.nofoundImg{
  display:flex;
  justify-content: center;
}
.nofoundImg image{
  width:182px;
  height:120px; 
  margin-top: 30%;
}
.nofoundText{
  display:flex;
  justify-content: center;
  text-align: center;
  font-size: 25rpx;
  margin-top: 15px;
  color: #515151;
}
button::after{ 
	border: none; 
}
.btn{
  display:flex;
  justify-content: center;
}
.btn button{
    margin-top: 15px;
    width: 19vw;
    height: 8vw;
    text-align: center;
    border-radius: 30px;
    background: white;
    color: #515151;
    font-size: 25rpx;
}